{
  "name": "@rbxts/sentry",
  "version": "1.2.2",
  "description": "An unofficial Roblox SDK for Sentry.io",
  "contributors": [
    {
      "name": "mutex-lock",
      "url": "https://github.com/mutex-lock"
    },
    {
      "name": "devSparkle",
      "url": "https://github.com/devSparkle"
    }
  ],
  "keywords": ["roblox", "rbxts", "sentry"],
  "homepage": "https://github.com/mutex-lock/sentry-roblox#readme",
  "bugs": "https://github.com/devSparkle/sentry-roblox/issues",
  "repository": "github:mutex-lock/sentry-roblox",
  "main": "out/init.lua",
  "types": "out/index.d.ts",
  "exports": {
    ".": "./out/index.d.ts"
  },
  "scripts": {
    "build": "rbxtsc",
    "watch": "rbxtsc -w",
    "prepublishOnly": "pnpm run build"
  },
  "files": [
    "out",
    "default.project.json",
    "!**/*.tsbuildinfo"
  ],
  "packageManager": "pnpm@10.12.4",
  "publishConfig": {
    "access": "public"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@rbxts/compiler-types": "3.0.0-types.0",
    "@rbxts/types": "^1.0.861",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-roblox-ts": "^0.0.36",
    "prettier": "^3.6.2",
    "roblox-ts": "^3.0.0",
    "typescript": "^5.8.3"
  }
}
